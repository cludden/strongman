{
  "extends": "airbnb-base",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": [
          "**/test/**/*.test.js"
        ]
      }
    ],
    "no-param-reassign": "off",
    "no-underscore-dangle": "off",
    "no-use-before-define": "off"
  }
}
